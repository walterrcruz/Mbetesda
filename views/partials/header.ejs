    <style>
        .demos-filter {
            margin: 0;
            text-align: right;
        }
        
        .demos-filter li {
            list-style: none;
            margin: 10px 0px;
        }
        
        .demos-filter li a {
            display: block;
            border: 0;
            text-transform: uppercase;
            letter-spacing: 1px;
            color: #444;
        }
        
        .demos-filter li a:hover,
        .demos-filter li.activeFilter a {
            color: #1ABC9C;
        }
        
        @media (max-width: 991px) {
            .demos-filter {
                text-align: center;
            }
            .demos-filter li {
                float: left;
                width: 33.3%;
                padding: 0 20px;
            }
        }
        
        @media (max-width: 767px) {
            .demos-filter li {
                width: 50%;
            }
        }
        
        #rev_slider_73_1 .uranus.tparrows {
            width: 50px;
            height: 50px;
            background: rgba(255, 255, 255, 0);
        }
        
        #rev_slider_73_1 .uranus.tparrows:before {
            width: 50px;
            height: 50px;
            line-height: 50px;
            font-size: 30px;
            transition: all 0.3s;
            -webkit-transition: all 0.3s;
        }
        
        #rev_slider_73_1 .uranus.tparrows:hover:before {
            opacity: 0.75;
        }
    </style>
