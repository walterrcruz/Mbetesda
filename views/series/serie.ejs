<% include ../partials/header %>


    <!-- Page Title
		============================================= -->
    <section id="page-title" class="page-title-parallax page-title-light" style="background-image: url('/assets/images2/stonebg.png'); padding: 100px 0;" data-stellar-background-ratio="0.9">

        <div class="container clearfix">
            <h1>Lista de Series</h1>
            <ol class="breadcrumb">
                <li><a href="/">Home</a></li>
                <li class="active"><a href="#">Serie</a></li>
                <li class="active"><a href="/serie/sermones">Lista de sermones</a></li>

            </ol>
        </div>

        <!-- Content
		============================================= -->
        <!-- Content
		============================================= -->
        <section id="content">

            <div class="content-wrap nopadding">

                <div class="section yt-bg-player nomargin dark" data-quality="hd1080" data-start="1250" data-stop="1270" data-video="<https://youtu.be/RSquCTVhha4>" style="height: 800px;">
                    <div class="container vertical-middle center clearfix">
                        <i class="i-plain i-large icon-line-video divcenter" data-animate="fadeInDown"></i>
                        <div class="emphasis-title nomargin" data-animate="fadeInUp">
                            <h2 style="font-size: 56px;">"SERIE ACTUAL: LAS ENSENANSAS DE JESUS"</h2>
                        </div>
                    </div>
                </div>


            </div>

        </section>
        <!-- #content end -->

        <section id="content">

            <div class="content-wrap">

                <div class="container clearfix">

                    <% if(!currentUser){ %>
                        <% } else { %>
                            <div class="container center">
                                <a href="/serie/new">Agregar Sermon</a>
                            </div>
                            <% } %>
                                <div class="line"></div>>
                                <!--============================================= -->
                                <ul id="portfolio-filter" class="portfolio-filter clearfix" data-container="#portfolio">

                                    <li class="activeFilter"><a href="#" data-filter="*">Show All</a></li>
                                    <li><a href="#" data-filter=".sermon">Sermones</a></li>
                                    <li><a href="#" data-filter=".pf-illustrations">Illustrations</a></li>
                                    <li><a href="#" data-filter=".pf-uielements">UI Elements</a></li>
                                    <li><a href="#" data-filter=".pf-media">Media</a></li>
                                    <li><a href="#" data-filter=".pf-graphics">Graphics</a></li>

                                </ul>
                                <!-- #portfolio-filter end -->
                                <div id="portfolio-shuffle" class="portfolio-shuffle" data-container="#portfolio">
                                    <i class="icon-random"></i>
                                </div>

                                <div class="clear"></div>

                                <!-- Posts
					============================================= -->
                                <div id="portfolio" class="portfolio grid-container portfolio-masonry clearfix">

                                    <% serie.forEach(function(sermon){ %>


                                        <article class="portfolio-item sermon">
                                            <ul>
                                                <div id="results">
                                                    <div class="portfolio-image">
                                                        <a href="/serie/<%= sermon._id %>">
                                                            <img src="/assets/images/portfolio/masonry/4/7.jpg" alt="Backpack Contents">
                                                        </a>
                                                        <div class="portfolio-overlay">
                                                            <a href="<%= sermon.video %>" class="left-icon" data-lightbox="iframe"><i class="icon-line-play"></i></a>
                                                            <a href="/serie/<%= sermon._id %>" class="right-icon">info<i class="icon-line-ellipsis"></i></a>
                                                        </div>
                                                    </div>
                                                    <div class="portfolio-desc">
                                                        <h3>
                                                            <a href="/serie/<%= sermon._id %>">
                                                                <%= sermon.title %>
                                                            </a>
                                                        </h3>
                                                        <span><a href="/serie/<%= sermon._id %>">Ministerios</a> <a href="/serie/<%= sermon._id %>">Betesda</a></span>
                                                    </div>
                                                </div>
                                            </ul>
                                        </article>
                                        <% }) %>

                                </div>
                                <!-- #posts end -->

                </div>


            </div>

        </section>
        <!-- #content end -->



        <% include ../partials/footer %>